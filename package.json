{
  "name": "ecommerce-mik",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18",
    "npm": ">=10"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "oxlint lint",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check --ignore-unknown .",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "commit-msg": "npm run commitmsg"
    }
  },
  "git": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.28.1",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "commitlint-config-gitmoji": "^2.3.1",
    "daisyui": "^4.4.20",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-unicorn": "^49.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "oxlint": "^0.0.21",
    "postcss": "^8",
    "prettier": "^3.1.1",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "simple-zustand-devtools": "^1.0.1",
    "standard": "^17.1.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
